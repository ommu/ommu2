{
	"name": "ommu/ommu2",
	"description": "Ommu2 Platform",
	"keywords": [
		"cms",
		"module",
		"backoffice-theme",
		"gii",
		"core",
		"ommu",
		"ommu2",
		"yii2"
	],
	"homepage": "https://github.com/ommu/ommu2",
	"type": "project",
	"license": "Apache-2.0",
	"support": {
		"issues": "https://github.com/ommu/ommu2/issues",
		"wiki": "https://github.com/ommu/ommu2/wiki",
		"source": "https://github.com/ommu/ommu2"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/ommu/ommu2.git"
	},
	"authors": [
		{
			"name": "Putra Sudaryanto",
			"email": "putra@ommu.id",
			"homepage": "https://ommu.id",
			"role": "Creator"
		}
	],
	"require": {
		"php": "~7.0",
		"oomphinc/composer-installers-extender": "^2.0",
		"yiisoft/yii2": "~2.0.14",
		"yiisoft/yii2-bootstrap": "~2.0.0",
		"yiisoft/yii2-bootstrap4": "~2.0.0",
		"yiisoft/yii2-swiftmailer": "~2.0.0",
		"yiisoft/yii2-redis": "^2.0",
		"mdmsoft/yii2-admin": "^2.7",
		"yiisoft/yii2-imagine": "~2.0",
		"yiisoft/yii2-jui": "^2.0",
        "yiisoft/yii2-queue": "^2.1",
		"sizeg/yii2-jwt": "^1.0",
		"thamtech/yii2-uuid": "*",
		"mg-code/yii2-helpers": "*",
		"adampatterson/numeral": "dev-master",
		"kartik-v/yii2-date-range": "dev-master",
		"npm-asset/pdfjs-dist": "~2.2",
        "victor78/yii2-zipper": "~0.0.4",
		"npm-asset/workbox-sw": "4.3.1",
        "predis/predis": "^1.1",
        "npm-asset/wolfy87-eventemitter": "~5.2.1",

		"ommu/yii2-redactor": "~2.0",
		"ommu/yii-traits": "v2.x-dev",
		"ommu/yii2-selectize": "dev-master",
		"ommu/yii2-flatpickr": "dev-master",
		"ommu/yii2-daterangepicker": "dev-master",
		"ommu/gapi-google-analytics-php-interface": "v2.x-dev",
		"ommu/yii2-centrifugo": "dev-master",

        "components/jquery": "3.5.*@stable | 3.4.*@stable | 3.3.*@stable | 3.2.*@stable | 3.1.*@stable | 2.2.*@stable | 2.1.*@stable | 1.11.*@stable | 1.12.*@stable",
        "npm-asset/inputmask": "~3.2.2 | ~3.3.5",
        "npm-asset/punycode": "1.3.*",
        "npm-asset/yii2-pjax": "~2.0.1",
        "components/bootstrap": "3.4.* | 3.3.* | 3.2.* | 3.1.*",
        "twitter/typeahead.js": "0.10.* | ~0.11.0",
        "components/jqueryui": "~1.12.1",

		"ommu/core": "v2.x-dev",
		"ommu/users": "v2.x-dev",
		"ommu/mailer": "dev-master",
		"ommu/report": "v2.x-dev",
		"ommu/support":  "v2.x-dev",
		"ommu/banner": "v2.x-dev",

		"ommuid/back3nd": "dev-master"
	},
	"require-dev": {
		"yiisoft/yii2-debug": "~2.0.0",
		"yiisoft/yii2-gii": "~2.0.0",
		"yiisoft/yii2-faker": "~2.0.0",
		"ommu/gii": "v2.x-dev",

		"codeception/base": "^2.2.3",
		"codeception/verify": "~0.3.1",
		"codeception/specify": "~0.4.3"
	},
	"config": {
		"process-timeout": 1800,
		"preferred-install": "dist",
		"vendor-dir": "protected/vendor",
		"fxp-asset": {
			"enabled": false
		}
	},
	"scripts": {
		"post-create-project-cmd": [
			"yii\\composer\\Installer::postCreateProject",
			"yii\\composer\\Installer::postInstall"
		],
		"post-install-cmd": [
			"yii\\composer\\Installer::postInstall"
		],
		"post-update-cmd": [
			"yii\\composer\\Installer::postInstall"
		]
	},
	"extra": {
		"yii\\composer\\Installer::postCreateProject": {

		},
		"yii\\composer\\Installer::postInstall": {
            "copyFiles": [
                {
					"author-sample.yaml": "author.yaml",
					"protected/config/main.php.sample": "protected/config/main.php",
					"protected/config/database.php.sample": "protected/config/database.php",
					"apps.json": "back3nd/apps.json"
                }
            ],
			"generateCookieValidationKey": [
				"protected/config/main.php"
			],
			"setPermission": [
				{
					"protected/runtime": "0777",
					"back3nd/assets": "0777",
					"back3nd/cache": "0777",
					"back3nd/app/runtime": "0777",
					"back3nd/apps.json": "0777"
				}
			]
		},
		"installer-types": [
			"ommu-theme",
			"ommu-apps"
		],
		"installer-paths": {
			"protected/themes/{$name}/": [
				"type:ommu-theme"
			],
			"{$name}": [
				"type:ommu-apps"
			]
		}
	},
    "suggest": {
        "bower-asset/jquery": "change to components/jquery",
        "bower-asset/inputmask": "change to npm-asset/inputmask",
        "bower-asset/punycode": "change to npm-asset/punycode",
        "bower-asset/yii2-pjax": "change to npm-asset/yii2-pjax",
        "bower-asset/bootstrap": "change to components/bootstrap",
        "bower-asset/jquery-ui": "change to components/jqueryui",
        "bower-asset/typeahead.js": "change to twitter/typeahead.js"
    },
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	]
}
